// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
model Code {
    id            String    @id @default(cuid())
    name          String?
    active        Boolean  
    
   
}

model Description {
    
    descriptionid            String    @id @default(cuid())
    descriptionName          String?
    active        Boolean  
    codeId        String?
    
   
}